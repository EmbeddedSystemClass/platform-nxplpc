{
  "build": {
    "cpu": "cortex-m4",
    "f_cpu": "120000000L",
    "mcu": "lpc4088"
  },
  "connectivity": [
    "can",
    "ethernet"
  ],
  "debug": {
    "jlink_device": "LPC4088",
    "tools": {
      "cmsis-dap": {
        "server": {
          "package": "tool-openocd",
          "executable": "bin/openocd",
          "arguments": [
            "-f", "scripts/interface/cmsis-dap.cfg",
            "-f", "scripts/target/lpc40xx.cfg"
          ]
        },
        "load_cmd": "preload",
        "onboard": true
      }
    }
  },
  "frameworks": [
    "mbed"
  ],
  "name": "Embedded Artists LPC4088 QuickStart Board",
  "upload": {
    "maximum_ram_size": 98304,
    "maximum_size": 524288,
    "protocols": [
      "jlink",
      "cmsis-dap",
      "mbed"
    ],
    "protocol": "mbed"
  },
  "url": "https://developer.mbed.org/platforms/EA-LPC4088/",
  "vendor": "Embedded Artists"
}