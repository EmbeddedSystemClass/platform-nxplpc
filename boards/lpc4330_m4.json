{
  "build": {
    "cpu": "cortex-m4", 
    "f_cpu": "204000000L", 
    "mcu": "lpc4330"
  }, 
  "debug": {
    "jlink_device": "LPC4330_M0", 
    "tools": {
      "cmsis-dap": {
        "load_cmd": "preload", 
        "onboard": true, 
        "server": {
          "arguments": [
            "-f", 
            "scripts/interface/cmsis-dap.cfg", 
            "-c", 
            "set CHIPNAME lpc43xx; set CHIPSERIES lpc4000; if { ![info exists WORKAREASIZE] } { set WORKAREASIZE 0x4000 }", 
            "-f", 
            "scripts/target/lpc1xxx.cfg"
          ], 
          "executable": "bin/openocd", 
          "package": "tool-openocd"
        }
      }
    }
  }, 
  "frameworks": [
    "mbed"
  ], 
  "name": "Bambino-210E", 
  "upload": {
    "maximum_ram_size": 270336, 
    "maximum_size": 8388608, 
    "protocol": "mbed", 
    "protocols": [
      "jlink", 
      "blackmagic", 
      "cmsis-dap", 
      "mbed"
    ]
  }, 
  "url": "https://developer.mbed.org/platforms/Micromint-Bambino-210E/", 
  "vendor": "Micromint"
}