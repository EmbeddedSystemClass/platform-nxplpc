{
  "build": {
    "cpu": "cortex-m4",
    "f_cpu": "120000000L",
    "mcu": "lpc4088"
  },
  "connectivity": [
    "can",
    "ethernet"
  ],
  "debug": {
    "jlink_device": "LPC4088",
    "svd_path": "LPC408x_7x_v0.7.svd",
    "tools": {
      "cmsis-dap": {
        "onboard": true,
        "server": {
          "arguments": [
            "pyocd-gdbserver.py",
            "-t",
            "lpc4088dm"
          ],
          "executable": "$PYTHONEXE",
          "package": "tool-pyocd"
        }
      }
    }
  },
  "frameworks": [
    "mbed"
  ],
  "name": "Embedded Artists LPC4088 Display Module",
  "upload": {
    "maximum_ram_size": 98304,
    "maximum_size": 524288,
    "protocol": "mbed",
    "protocols": [
      "jlink",
      "cmsis-dap",
      "mbed"
    ]
  },
  "url": "https://developer.mbed.org/platforms/EA-LPC4088-Display-Module/",
  "vendor": "Embedded Artists"
}
