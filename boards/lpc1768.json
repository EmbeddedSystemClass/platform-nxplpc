{
  "build": {
    "cpu": "cortex-m3",
    "f_cpu": "96000000L",
    "mcu": "lpc1768"
  },
  "connectivity": [
    "can",
    "ethernet"
  ],
  "debug": {
    "jlink_device": "LPC1768",
    "tools": {
      "cmsis-dap": {
        "onboard": true,
        "server": {
          "arguments": [
            "pyocd-gdbserver.py",
            "-t",
            "lpc1768"
          ],
          "executable": "$PYTHONEXE",
          "package": "tool-pyocd"
        }
      }
    }
  },
  "frameworks": [
    "mbed"
  ],
  "name": "NXP mbed LPC1768",
  "upload": {
    "maximum_ram_size": 65536,
    "maximum_size": 524288,
    "protocol": "mbed",
    "protocols": [
      "jlink",
      "blackmagic",
      "cmsis-dap",
      "mbed"
    ]
  },
  "url": "http://developer.mbed.org/platforms/mbed-LPC1768/",
  "vendor": "NXP"
}