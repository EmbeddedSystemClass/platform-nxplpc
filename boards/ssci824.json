{
  "build": {
    "cpu": "cortex-m0plus",
    "f_cpu": "30000000L",
    "mcu": "lpc824"
  },
  "debug": {
    "jlink_device": "LPC824M201",
    "tools": {
      "cmsis-dap": {
        "onboard": true,
        "server": {
          "arguments": [
            "pyocd-gdbserver.py",
            "-t",
            "lpc824"
          ],
          "executable": "$PYTHONEXE",
          "package": "tool-pyocd"
        }
      }
    }
  },
  "frameworks": [
    "mbed"
  ],
  "name": "Switch Science mbed LPC824",
  "upload": {
    "maximum_ram_size": 8192,
    "maximum_size": 32768,
    "protocol": "mbed",
    "protocols": [
      "jlink",
      "blackmagic",
      "cmsis-dap",
      "mbed"
    ]
  },
  "url": "https://developer.mbed.org/platforms/Switch-Science-mbed-LPC824/",
  "vendor": "Switch Science"
}
