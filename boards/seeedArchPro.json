{
  "build": {
    "cpu": "cortex-m3",
    "f_cpu": "96000000L",
    "mcu": "lpc1768"
  },
  "connectivity": [
    "ethernet"
  ],
  "debug": {
    "tools": {
      "cmsis-dap": {
        "load_cmd": "preload",
        "onboard": true,
        "server": {
          "arguments": [
            "-f",
            "scripts/interface/cmsis-dap.cfg",
            "-f",
            "scripts/target/lpc17xx.cfg"
          ],
          "executable": "bin/openocd",
          "package": "tool-openocd"
        }
      }
    }
  },
  "frameworks": [
    "mbed"
  ],
  "name": "Seeed Arch Pro",
  "upload": {
    "maximum_ram_size": 65536,
    "maximum_size": 524288,
    "protocol": "mbed",
    "protocols": [
      "cmsis-dap",
      "mbed"
    ]
  },
  "url": "https://developer.mbed.org/platforms/Seeeduino-Arch-Pro/",
  "vendor": "SeeedStudio"
}